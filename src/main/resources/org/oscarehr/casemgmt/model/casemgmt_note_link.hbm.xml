<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.oscarehr.casemgmt.model.CaseManagementNoteLink" table="casemgmt_note_link">
		<id name="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property column="table_name" name="tableName" />
		<property column="table_id" name="tableId" />
		<property column="note_id" name="noteId" />
		<property column="other_id" name="otherId" />
	</class>
</hibernate-mapping>
