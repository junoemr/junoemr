<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="ProgramClientStatus" table="program_clientstatus">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="integer" column="clientstatus_id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="string" not-null="false"
			length="255" />
		<property name="programId" column="program_id" type="integer"
			not-null="false" length="10" />
	</class>
</hibernate-mapping>
